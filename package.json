{
	"name": "superstarter",
	"version": "1.0.0",
	"private": false,
	"type": "module",
	"scripts": {
		"dev": "bun --bun next dev",
		"dev:inngest": "bun --bun inngest-cli dev --queue-workers 4000 --poll-interval 1",
		"build": "bun --bun next build",
		"start": "bun --bun next start",
		"lint": "bun --bun biome lint --write --staged --no-errors-on-unmatched && bun run scripts/dev/lint.ts --staged",
		"lint:all": "bun --bun biome lint --write && bun run scripts/dev/lint.ts",
		"style": "bun run scripts/dev/style.ts",
		"format": "bun run scripts/dev/fmt.ts --strip-comments --write && bun --bun biome format --write",
		"typecheck": "bun --bun tsc",
		"precommit": "bun --bun biome check --staged --no-errors-on-unmatched && bun run scripts/dev/lint.ts --staged && bun typecheck",
		"prepare": "bunx simple-git-hooks",
		"db:generate": "bun --bun drizzle-kit generate",
		"db:migrate": "bun --bun drizzle-kit migrate",
		"db:push": "bun --bun drizzle-kit push",
		"db:studio": "bun --bun drizzle-kit studio",
		"db:drop": "bun --bun run src/db/scripts/drop-schemas.ts",
		"db:apply": "bun --bun run src/db/scripts/apply-programs.ts"
	},
	"dependencies": {
		"@ai-sdk/anthropic": "^3.0.46",
		"@ai-sdk/openai": "^3.0.30",
		"@base-ui/react": "^1.2.0",
		"@chat-adapter/slack": "^4.14.0",
		"@chat-adapter/state-memory": "^4.14.0",
		"@clerk/nextjs": "^6.38.2",
		"@dnd-kit/core": "^6.3.1",
		"@dnd-kit/modifiers": "^9.0.0",
		"@dnd-kit/sortable": "^10.0.0",
		"@dnd-kit/utilities": "^3.2.2",
		"@floating-ui/dom": "^1.7.5",
		"@inngest/agent-kit": "^0.13.2",
		"@inngest/realtime": "^0.4.6",
		"@superbuilders/errors": "^3.0.2",
		"@superbuilders/slog": "^4.0.0",
		"@t3-oss/env-nextjs": "^0.13.10",
		"@tiptap/core": "^3.20.0",
		"@tiptap/extension-code-block-lowlight": "^3.20.0",
		"@tiptap/extension-list": "^3.20.0",
		"@tiptap/extension-subscript": "^3.20.0",
		"@tiptap/extension-superscript": "^3.20.0",
		"@tiptap/extension-table": "^3.20.0",
		"@tiptap/extension-text-style": "^3.20.0",
		"@tiptap/extension-typography": "^3.20.0",
		"@tiptap/extensions": "^3.20.0",
		"@tiptap/markdown": "^3.20.0",
		"@tiptap/pm": "^3.20.0",
		"@tiptap/react": "^3.20.0",
		"@tiptap/starter-kit": "^3.20.0",
		"@tiptap/suggestion": "^3.20.0",
		"@vercel/sandbox": "^1.6.0",
		"ai": "^6.0.97",
		"chat": "^4.14.0",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"cmdk": "^1.1.1",
		"drizzle-orm": "^0.45.1",
		"fuse.js": "^7.1.0",
		"inngest": "^3.52.0",
		"lowlight": "^3.3.0",
		"lucide-react": "^0.575.0",
		"motion": "^12.34.3",
		"next": "^16.1.6",
		"next-themes": "^0.4.6",
		"openapi-fetch": "^0.17.0",
		"radix-ui": "^1.4.3",
		"react": "^19.2.4",
		"react-dom": "^19.2.4",
		"react-markdown": "^10.1.0",
		"remark-gfm": "^4.0.1",
		"shadcn": "^3.8.4",
		"sonner": "^2.0.7",
		"tailwind-merge": "^3.4.1",
		"tippy.js": "^6.3.7",
		"tldraw": "^4.4.0",
		"tw-animate-css": "^1.4.0",
		"zod": "^4.3.6"
	},
	"devDependencies": {
		"@anthropic-ai/claude-agent-sdk": "^0.2.42",
		"@biomejs/biome": "^2.4.0",
		"@tailwindcss/postcss": "^4.1.18",
		"@types/bun": "^1.3.9",
		"@types/node": "^25.2.3",
		"@types/react": "^19.2.14",
		"@types/react-dom": "^19.2.3",
		"drizzle-kit": "^0.31.9",
		"inngest-cli": "^1.17.0",
		"openapi-typescript": "^7.13.0",
		"postcss": "^8.5.6",
		"simple-git-hooks": "^2.13.1",
		"tailwindcss": "^4.1.18",
		"typescript": "^5.9.3"
	},
	"ct3aMetadata": {
		"initVersion": "7.39.3"
	},
	"simple-git-hooks": {
		"pre-commit": "bun run precommit"
	},
	"trustedDependencies": [
		"@biomejs/biome",
		"@tailwindcss/oxide",
		"inngest-cli",
		"simple-git-hooks"
	]
}
